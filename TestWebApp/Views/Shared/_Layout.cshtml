<!DOCTYPE HTML>
<html>
<head>
    @await Html.PartialAsync("_MetatagsPartials")
    @await Html.PartialAsync("_CssPartial")  
</head>
<body>
    <div id="page-wrapper">
        @await Html.PartialAsync("_HeaderPartial")
        <div id="main">
            <div class="container">
                <div class="row main-row">
                    <div class="col-8 col-12-medium">
                        @RenderBody()
                    </div>
                </div>
                @await Html.PartialAsync("_SidebarPartial") 
            </div>
        </div>
        @await Html.PartialAsync("_FooterPartial.cshtml");
    </div>
    @await Html.PartialAsync("_ScriptsPartial") 
</body>
</html>
